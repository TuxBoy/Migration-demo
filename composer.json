{
    "name": "tuxboy/auto-update",
    "authors": [
        {
            "name": "TuxBoy",
            "email": "benoit.mallo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTest\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "tuxboy/sdam": "dev-master",
        "slim/slim": "^3.10",
        "slim/twig-view": "^2.4",
        "doctrine/dbal": "^2.8",
        "cocur/slugify": "^3.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:tuxboy/sdam.git"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^4.1",
        "phpunit/phpunit": "^7.2",
        "zeuxisoo/slim-whoops": "^0.6.4"
    }
}
